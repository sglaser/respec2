<div class="head">
{{#if prependLogo}}
<p>
    <a href="https://www.nvidia.com/">
    {{#if useLogo}}
        <img width="270" height="230" alt="Nvidia Logo" src="{{useLogo}}"/>
    {{else}}
        <img width="270" height="230" alt="Nvidia Logo" src="https://sglaser.github.io/respec/stylesheets/NV/NVLogo_2D.svg"/>
    {{/if}}
    </a>
</p>
{{/if}}
    
<div id="respec-banner">
    <span id="respec-banner-status">{{specStatusLong}}</span>&nbsp;&mdash;&nbsp;
    {{#if publishHumanDate}}
        <span id="respec-banner-maturity">{{publishHumanDate}}</span>&nbsp;&mdash;&nbsp;
    {{/if}}
    <span id="respec-banner-spec-name">{{title}}</span>&nbsp;&nbsp;&nbsp;
</div>
    
<h1 class="title p-name" id="title"{{#if doRDFa}} property="dcterms:title"{{/if}}>{{title}}</h1>
    
{{#if subtitle}}
    <h2 {{#if doRDFa}}property="bibo:subtitle" {{/if}}id="subtitle" class="nolink">{{subtitle}}</h2>
{{/if}}
    
<h2 {{#if doRDFa}}property="dcterms:issued" datatype="xsd:dateTime"
    content="{{publishISODate}}"{{/if}} class="nolink">
    <time class="dt-published" datetime="{{dashDate}}">{{publishHumanDate}}</time>
</h2>
    
<dl>
    {{#if thisVersion}}
        <dt>This version:</dt>
        <dd><a class="u-url" href="{{thisVersion}}">{{thisVersion}}</a></dd>
    {{/if}}
    
    {{#if latestVersion}}
        <dt>Latest published version:</dt>
        <dd><a href="{{latestVersion}}">{{latestVersion}}</a></dd>
    {{/if}}

    {{#if edDraftURI}}
        <dt>Latest editor"s draft:</dt>
        <dd><a href="{{edDraftURI}}">{{edDraftURI}}</a></dd>
    {{/if}}

    {{#if testSuiteURI}}
        <dt>Test suite:</dt>
        <dd><a href="{{testSuiteURI}}">{{testSuiteURI}}</a></dd>
    {{/if}}

    {{#if implementationReportURI}}
        <dt>Implementation report:</dt>
        <dd><a href="{{implementationReportURI}}">{{implementationReportURI}}</a></dd>
    {{/if}}

    {{#if bugTrackerHTML}}
        <dt>Bug tracker:</dt>
        <dd>{{{bugTrackerHTML}}}</dd>
    {{/if}}

    {{#if showPreviousVersion}}
        <dt>Previous version:</dt>
        <dd><a {{#if doRDFa}}rel="dcterms:replaces" {{/if}}href="{{prevVersion}}">{{prevVersion}}</a></dd>
    {{/if}}

    {{#if editors}}
        <dt>Editor{{#if multipleEditors}}s{{/if}}:</dt>
        {{showPeople "Editor" editors}}
    {{/if}}

    {{#if authors}}
        <dt>Author{{#if multipleAuthors}}s{{/if}}:</dt>
        {{showPeople "Author" authors}}
    {{/if}}

    {{#if otherLinks}}
        {{#each otherLinks}}
            {{#if key}}
                <dt {{#if class}}class="{{class}}"{{/if}}>{{key}}:</dt>
                {{#if data}}
                    {{#each data}}
                        {{#if value}}
                            <dd {{#if class}}class="{{class}}"{{/if}}>
                                {{#if href}}<a href="{{href}}">{{/if}}{{value}}{{#if href}}</a>{{/if}}
                            </dd>
                        {{else}}
                            {{#if href}}
                                <dd><a href="{{href}}">{{href}}</a></dd>
                            {{/if}}
                        {{/if}}
                    {{/each}}
                {{else}}
                    {{#if value}}
                        <dd {{#if class}}class="{{class}}"{{/if}}>
                            {{#if href}}<a href="{{href}}">{{/if}}{{value}}{{#if href}}</a>{{/if}}
                        </dd>
                    {{else}}
                        {{#if href}}
                            <dd {{#if class}}class="{{class}}"{{/if}}><a href="{{href}}">{{href}}</a></dd>
                        {{/if}}
                    {{/if}}
                {{/if}}
            {{/if}}
        {{/each}}
    {{/if}}

</dl>
    
{{#if errata}}
<p>
    Please check the <a href="{{errata}}"><strong>{{errata}}</strong></a> for any errors or issues
    reported since publication.
</p>
{{/if}}
    
{{#if alternateFormats}}
<p>
    {{#if multipleAlternates}}
    This document is also available in these non-normative formats:
    {{else}}
    This document is also available in this non-normative format:
    {{/if}}
    {{{alternatesHTML}}}
</p>
{{/if}}

{{#if overrideCopyright}}
    {{{overrideCopyright}}}
{{else}}
    <p class="copyright">
        Copyright &copy; {{#if copyrightStart}}{{copyrightStart}}-{{/if}}{{publishYear}} {{#if
        additionalCopyrightHolders}} {{{additionalCopyrightHolders}}} &amp;{{/if}}
        <a href="https://www.nvidia.com/">Nvidia, Inc</a>
        <sup>&reg;</sup>
    </p>
{{/if}}

    <hr/>
</div>
