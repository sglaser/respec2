<div class="head">
{{#if logos}}
    {{showLogos logos}}
{{else}}
    {{#if prependPCIeLogo}}
    <p>
        <a href="https://www.pcisig.com/">
            <img width="210" height="80" alt="PCI Express Logo"
                 src="https://sglaser.github.io/respec/stylsheets/pcisig/pci_express_PMS.svg"/>
        </a>
    </p>
    {{/if}}
    {{#if prependPCISIGLogo}}
    <p>
        <a href="https://www.pcisig.com/">
            <img width="210" height="108" alt="PCISIG Logo"
                 src="https://sglaser.github.io/respec/stylsheets/pcisig/pci_sig_logo_PMS_273.svg"/>
        </a>
    </p>
    {{/if}}
{{/if}}
<div id="respec-banner">
    <span id="respec-banner-status">{{specStatusLong}}</span>&nbsp;&mdash;&nbsp;
    {{#if specReviewLong}}
        <span id="respec-banner-next-state">{{specReviewLong}}</span>&nbsp;&mdash;&nbsp;
    {{/if}}
    {{#if specLevelLong}}
        <span id="respec-banner-maturity">{{specLevelLong}}</span>&nbsp;&mdash;&nbsp;
    {{/if}}
    <span id="respec-banner-spec-name">{{title}}</span>&nbsp;&nbsp;&nbsp;
</div>
<h1 class="title p-name" id="title"{{#if doRDFa}} property="dcterms:title"{{/if}}>{{title}}</h1>
{{#if subtitle}}
    <h2 {{#if doRDFa}}property="bibo:subtitle" {{/if}}id="subtitle" class="nolink">{{subtitle}}</h2>
{{/if}}
<h2 {{#if doRDFa}}property="dcterms:issued" datatype="xsd:dateTime"
content="{{publishISODate}}"{{/if}} class="nolink">
<time class="dt-published" datetime="{{dashDate}}">{{publishHumanDate}}</time>
</h2>
<dl>
    {{#unless isNoTrack}}
    <dt>This version:</dt>
        <dd>{{#if thisVersion}}<a class="u-url" href="{{thisVersion}}">{{thisVersion}}</a>{{else}}none{{/if}}}</dd>
    <dt>Latest published version:</dt>
    <dd>{{#if latestVersion}}<a class="u.url" href="{{latestVersion}}">{{latestVersion}}</a>{{else}}none{{/if}}
    </dd>
    {{/unless}}
    {{#if edDraftURI}}
    <dt>Latest editor's draft:</dt>
    <dd><a class="u-url" href="{{edDraftURI}}">{{edDraftURI}}</a></dd>
    {{/if}}
    {{#if testSuiteURI}}
    <dt>Test suite:</dt>
    <dd><a class="u-url" href="{{testSuiteURI}}">{{testSuiteURI}}</a></dd>
    {{/if}}
    {{#if implementationReportURI}}
    <dt>Implementation report:</dt>
    <dd><a class="u-url" href="{{implementationReportURI}}">{{implementationReportURI}}</a></dd>
    {{/if}}
    {{#if bugTrackerHTML}}
    <dt>Bug tracker:</dt>
    <dd>{{{bugTrackerHTML}}}</dd>
    {{/if}}
    {{#if isED}}
    {{#if prevED}}
    <dt>Previous editor's draft:</dt>
    <dd><a class="u-url" href="{{prevED}}">{{prevED}}</a></dd>
    {{/if}}
    {{/if}}
    {{#if showPreviousVersion}}
    <dt>Previous version:</dt>
    <dd><a {{#if doRDFa}}rel="dcterms:replaces"{{/if}}
            class="u-url" href="{{prevVersion}}">{{prevVersion}}</a></dd>
    {{/if}}
    {{#if prevRecURI}}
    {{#if isRec}}
    <dt>Previous Recommendation:</dt>
    <dd><a {{#if doRDFa}}rel="dcterms:replaces"{{/if}} href="{{prevRecURI}}">{{prevRecURI}}</a>
    </dd>
    {{else}}
    <dt>Latest Recommendation:</dt>
    <dd><a class="u-url" href="{{prevRecURI}}">{{prevRecURI}}</a></dd>
    {{/if}}
    {{/if}}
    <dt>Editor{{#if multipleEditors}}s{{/if}}:</dt>
    {{showPeople "Editor" editors}}
    {{#if authors}}
    <dt>Author{{#if multipleAuthors}}s{{/if}}:</dt>
    {{showPeople "Author" authors}}
    {{/if}}
    {{#if otherLinks}}
    {{#each otherLinks}}
    {{#if key}}
    <dt
    {{#if class}}class="{{class}}"{{/if}}>{{key}}:</dt>
    {{#if data}}
    {{#each data}}
    {{#if value}}
    <dd
    {{#if class}}class="{{class}}"{{/if}}>
    {{#if href}}<a href="{{href}}">{{/if}}
    {{value}}
    {{#if href}}</a>{{/if}}
    </dd>
    {{else}}
    {{#if href}}
    <dd><a href="{{href}}">{{href}}</a></dd>
    {{/if}}
    {{/if}}
    {{/each}}
    {{else}}
    {{#if value}}
    <dd
    {{#if class}}class="{{class}}"{{/if}}>
    {{#if href}}<a href="{{href}}">{{/if}}
    {{value}}
    {{#if href}}</a>{{/if}}
    </dd>
    {{else}}
    {{#if href}}
    <dd
    {{#if class}}class="{{class}}"{{/if}}>
    <a href="{{href}}">{{href}}</a>
    </dd>
    {{/if}}
    {{/if}}
    {{/if}}
    {{/if}}
    {{/each}}
    {{/if}}
</dl>
{{#if errata}}
<p>
    Please check the <a href="{{errata}}"><strong>errata</strong></a> for any errors or issues
    reported since publication.
</p>
{{/if}}
{{#if alternateFormats}}
<p>
    {{#if multipleAlternates}}
    This document is also available in these non-normative formats:
    {{else}}
    This document is also available in this non-normative format:
    {{/if}}
    {{{alternatesHTML}}}
</p>
{{/if}}
